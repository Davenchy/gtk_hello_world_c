<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object id="window" class="GtkWindow">
		<property name="title">Hello World</property>
		<child>
			<object id="grid" class="GtkGrid">
				<child>
					<object class="GtkButton" id="home-btn">
						<property name="label">Home</property>
						<layout>
							<property name="column">0</property>
							<property name="row">0</property>
						</layout>
					</object>
				</child>
				<child>
					<object class="GtkButton" id="settings-btn">
						<property name="label">Settings</property>
						<layout>
							<property name="column">1</property>
							<property name="row">0</property>
						</layout>
					</object>
				</child>
				<child>
					<object class="GtkButton" id="about-btn">
						<property name="label">About</property>
						<layout>
							<property name="column">2</property>
							<property name="row">0</property>
						</layout>
					</object>
				</child>
				<child>
					<object class="GtkButton" id="help-btn">
						<property name="label">Help</property>
						<layout>
							<property name="column">0</property>
							<property name="row">1</property>
						</layout>
					</object>
				</child>
				<child>
					<object class="GtkButton" id="quit-btn">
						<property name="label">Quit</property>
						<layout>
							<property name="column">1</property>
							<property name="row">1</property>
							<property name="column-span">2</property>
						</layout>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>
